# AI Website Builder Environment Configuration for Cozyartz Media Group
# Copy this file to .env.local and fill in your Cloudflare credentials

# Required: Cloudflare Workers AI Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here

# Optional: Fallback AI Services (for advanced features)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Base path for client site migration (usually leave as /)
BASE_PATH=/

# Development vs Production mode
NODE_ENV=development

# Cloudflare Pages Configuration
PAGES_PROJECT_NAME=wantthissite